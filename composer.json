{
    "name": "city-of-helsinki/asuntotuotanto",
    "description": "Drupal 9 Asuntotuotanto created by helfi drupal platform",
    "type": "project",
    "license": "MIT",
    "homepage": "https://druid.fi",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "asuntomyynti/react": "^1.3",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.6.7",
        "dinbror/blazy": "^1.8",
        "druidfi/omen": "^0.2.0",
        "drupal/api_tools": "^1.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "~6.0.3",
        "drupal/better_passwords": "^2.2",
        "drupal/better_social_sharing_buttons": "^4.0",
        "drupal/coffee": "^1.3",
        "drupal/computed_field_plugin": "^1.4",
        "drupal/config_terms": "^1.9",
        "drupal/contact_emails": "^1.21",
        "drupal/contact_storage": "^1.3",
        "drupal/context": "^5.0@RC",
        "drupal/core": "10.2.2",
        "drupal/core-composer-scaffold": "10.2.2",
        "drupal/email_registration": "^1.4",
        "drupal/externalauth": "^2.0",
        "drupal/field_group": "^3.1",
        "drupal/gdpr_compliance": "^1.22",
        "drupal/gin_login": "^2.0",
        "drupal/hdbt": "^6.0",
        "drupal/hdbt_admin": "^3.0",
        "drupal/helfi_azure_fs": "^2.0",
        "drupal/helfi_platform_config": "^4.0",
        "drupal/helfi_tunnistamo": "^2.0",
        "drupal/honeypot": "^2.1",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_slider": "~2.0.0",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/key": "^1.17",
        "drupal/mailsystem": "^4.4",
        "drupal/matomo": "^1.23",
        "drupal/matomo_reports": "^1.4",
        "drupal/menu_block": "^1.10",
        "drupal/nocurrent_pass": "^1.3",
        "drupal/rabbit_hole": "1.0-beta11",
        "drupal/raven": "^3.2",
        "drupal/rebuild_cache_access": "^1.9",
        "drupal/redis": "^1.7",
        "drupal/registration_role": "^2.0",
        "drupal/restui": "1.21",
        "drupal/route_condition": "2.0.2",
        "drupal/samlauth": "^3.9",
        "drupal/slick": "^2.10",
        "drupal/swiftmailer": "^2.4",
        "drupal/ultimate_cron": "2.0-alpha6",
        "drupal/user_bundle": "^1.2",
        "drupal/user_registrationpassword": "2.0.0",
        "drupal/yearonly": "^9.1",
        "drush/drush": "^12",
        "kenwheeler/slick": "^1.8",
        "symfony/console": "^6.3",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "10.2.2",
        "drupal/devel": "^5.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9.5",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./public"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "public/core": [
                "type:drupal-core"
            ],
            "public/libraries/{$name}": [
                "type:drupal-library"
            ],
            "public/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "public/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "public/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "public/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "public/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "https://www.drupal.org/project/drupal/issues/3054641#comment-13385850": "https://www.drupal.org/files/issues/2019-12-10/3054641-14.patch",
                "Views attachments are not rendered in the specified order [https://www.drupal.org/project/drupal/issues/2943293]": "https://www.drupal.org/files/issues/2019-05-25/2943293-17.patch",
                "https://www.drupal.org/project/drupal/issues/3098307": "https://www.drupal.org/files/issues/2022-07-01/sql_mode_fullgroupby_issue-3098307-15.patch"
            },
            "drupal/elasticsearch_connector": {
                "Custom patch based on https://www.drupal.org/files/issues/2020-03-03/8.x-7.x-2977537-13-array-vs-scalar_0.patch": "patches/elasticsearch_connector_indexfactory_v3.patch",
                "https://www.drupal.org/project/elasticsearch_connector/issues/2850749": "https://www.drupal.org/files/issues/2021-12-01/cannot-load-cluster-exception-2850749-10.patch"
            },
            "drupal/field_group": {
                "Custom patch to add classes for each horizontal tab": "patches/field_group_horizontal_tabs_classes.patch"
            },
            "drupal/user_bundle": {
                "Custom patch prevents install from dying when trying to edit nonexisting admin configuration file.": "./patches/remove_editing_admin_configuration.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repository.drupal.hel.ninja"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/kenwheeler/slick",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "asuntomyynti/react",
                "version": "1.3.4",
                "dist": {
                    "url": "https://github.com/City-of-Helsinki/asuntomyynti-react/releases/download/v1.3.4/asuntomyynti-react-1.3.4.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/dinbror/blazy",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "scripts": {
        "test-php": "vendor/bin/phpunit -c $PWD/phpunit.xml.dist",
        "copy-commit-message-script": "make copy-commit-message-script",
        "post-install-cmd": [
            "rsync -a vendor/asuntomyynti/react public/modules/custom/asu_apartment_search/assets || true",
            "@copy-commit-message-script"
        ],
        "post-update-cmd": [
            "rsync -a vendor/asuntomyynti/react public/modules/custom/asu_apartment_search/assets || true",
            "@copy-commit-message-script"
        ]
    }
}
