{
    "name": "city-of-helsinki/asuntotuotanto",
    "description": "Drupal 11 Asuntotuotanto created by helfi drupal platform",
    "type": "project",
    "license": "MIT",
    "homepage": "https://druid.fi",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "asuntomyynti/react": "^1.5",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^2.0",
        "dinbror/blazy": "^1.8",
        "druidfi/omen": "^0.9.0",
        "drupal/api_tools": "^1.0",
        "drupal/auto_entitylabel": "^3.4",
        "drupal/autologout": "^2.0",
        "drupal/better_exposed_filters": "^7.1",
        "drupal/better_passwords": "^2.2",
        "drupal/better_social_sharing_buttons": "^4.1",
        "drupal/big_pipe_sessionless": "^2.2",
        "drupal/computed_field_plugin": "^1.4",
        "drupal/config_filter": "^2.7",
        "drupal/config_terms": "^1.9",
        "drupal/context": "^5.0@RC",
        "drupal/core": "~11.2.0",
        "drupal/core-composer-scaffold": "~11.2.0",
        "drupal/core-recommended": "~11.2.0",
        "drupal/ctools": "^4.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/email_registration": "^1.4",
        "drupal/encrypt": "^3.2",
        "drupal/externalauth": "^2.0",
        "drupal/field_group": "^4.0",
        "drupal/flysystem": "^2.3",
        "drupal/gdpr_compliance": "^1.25",
        "drupal/gin": "^5.0",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^3.0",
        "drupal/hal": "^2.0",
        "drupal/hdbt": "^6.16",
        "drupal/hdbt_admin": "^3.4",
        "drupal/helfi_api_base": "^2.8",
        "drupal/helfi_azure_fs": "^2.2",
        "drupal/helfi_platform_config": "^5.0",
        "drupal/helfi_tunnistamo": "^3.1",
        "drupal/honeypot": "^2.2",
        "drupal/imagemagick": "^4.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/js_cookie": "^1.0",
        "drupal/key": "^1.21",
        "drupal/login_history": "^2.0",
        "drupal/mailsystem": "^4.5",
        "drupal/matomo": "^1.26",
        "drupal/matomo_reports": "^1.6",
        "drupal/menu_block": "^1.14",
        "drupal/monolog": "^3.0",
        "drupal/nocurrent_pass": "^2.0",
        "drupal/publication_date": "^3.1",
        "drupal/rabbit_hole": "^1.0",
        "drupal/raven": "^6.3",
        "drupal/rdf": "^3.0@beta",
        "drupal/rebuild_cache_access": "^1.12",
        "drupal/redis": "^1.11",
        "drupal/registration_role": "^2.0",
        "drupal/restui": "^1.22",
        "drupal/route_condition": "^2.0",
        "drupal/samlauth": "^3.13",
        "drupal/siteimprove": "^3.0",
        "drupal/slick": "^3.0",
        "drupal/symfony_mailer_lite": "^2.0",
        "drupal/tfa": "^1.12",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/upgrade_status": "^4.3",
        "drupal/user_bundle": "^1.4",
        "drupal/user_registrationpassword": "^2.0",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/yearonly": "^9.1",
        "drush/drush": "^13.7",
        "kenwheeler/slick": "^1.8",
        "mglaman/composer-drupal-lenient": "^1.0",
        "onelogin/php-saml": "^4.3",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "drupal/coder": "^8.3",
        "drupal/devel": "^5.0",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true,
            "zaporylie/composer-drupal-optimizations": true,
            "mglaman/composer-drupal-lenient": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./public"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[web-root]/robots.txt": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "public/core": [
                "type:drupal-core"
            ],
            "public/libraries/{$name}": [
                "type:drupal-library"
            ],
            "public/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "public/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "public/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "public/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "public/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/helfi_platform_config": {
                "Catch exception during hero paragraphs allowed values update": "patches/helfi_platform_config_hero_paragraph_fix.patch"
            },
            "drupal/config_terms": {
                "Drupal 11 compatibility fixes": "https://www.drupal.org/files/issues/2024-06-02/config_terms.1.x-dev.rector.patch"
            },
            "drupal/computed_field_plugin": {
                "Drupal 11 compatibility fixes": "patches/computed_field_plugin-d11.patch"
            },
            "drupal/core": {
                "[#UHF-12709] Patch 'Deleted and replaced ...' error messages": "patches/uhf-12709-skip-deleted-log.patch",
                "[#3452852] Add create() factory method with autowired parameters to PluginBase": "patches/3452852.patch",
                "[D11] Skip delete when config entity missing": "patches/core-import-delete-null.patch",
                "[D11] Skip delete in checkOp when entity missing": "patches/core-config-importer-null-entity.patch",
                "[D11] Skip countFieldData when table missing": "patches/core-count-field-data-missing-table.patch",
                "[D11] Skip delete when field table missing": "patches/core-skip-delete-missing-field-table.patch"
            },
            "drupal/elasticsearch_connector": {
                "[Local] Disable reopen parameter for ES": "patches/elasticsearch-connector-disable-reopen.patch"
            }
        },
        "composer-patches": {
            "ignore-dependency-patches": [
                "drupal/helfi_platform_config",
                "drupal/big_pipe_sessionless"
            ]
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/config_terms",
                "drupal/computed_field_plugin",
                "drupal/config_terms",
                "drupal/computed_field_plugin",
                "drupal/big_pipe_sessionless"
            ]
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/big_pipe_sessionless",
                "version": "2.2.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/project/big_pipe_sessionless.git",
                    "type": "git",
                    "reference": "2.2.0"
                },
                "require": {
                    "drupal/core": "^10 || ^11"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/computed_field_plugin",
                "version": "1.4.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/project/computed_field_plugin.git",
                    "type": "git",
                    "reference": "8.x-1.4"
                },
                "require": {
                    "drupal/core": "^10 || ^11"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/config_terms",
                "version": "1.9.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/project/config_terms.git",
                    "type": "git",
                    "reference": "8.x-1.9"
                },
                "require": {
                    "drupal/core": "^10 || ^11"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://repository.drupal.hel.ninja"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/kenwheeler/slick",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "asuntomyynti/react",
                "version": "1.5.0",
                "dist": {
                    "url": "https://github.com/City-of-Helsinki/asuntomyynti-react/releases/download/v1.5.0/asuntomyynti-react-1.5.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/dinbror/blazy",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "scripts": {
        "test-php": "vendor/bin/phpunit -c $PWD/phpunit.xml.dist",
        "copy-commit-message-script": "make copy-commit-message-script || true",
        "post-install-cmd": [
            "rsync -a vendor/asuntomyynti/react public/modules/custom/asu_apartment_search/assets || true",
            "@copy-commit-message-script"
        ],
        "post-update-cmd": [
            "rsync -a vendor/asuntomyynti/react public/modules/custom/asu_apartment_search/assets || true",
            "@copy-commit-message-script"
        ]
    }
}
